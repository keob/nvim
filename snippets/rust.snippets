snippet let "let"
	let ${1:pat} = ${0:expr};

snippet cst "const"
	const ${1:name}: ${2:type} = ${0:init};

snippet sts "static"
	static ${1:name}: ${2:type} = ${0:init};

snippet main "main function"
	fn main() {
		${0}
	}

snippet fn "function"
	fn ${1:name}(${2:arg}: ${3:type}) -> ${4:ret} {
		${0}
	}

snippet if "if block"
	if ${1:condition} {
		${0}
	}

snippet elif "else block"
	else if ${1:condition} {
		${0}
	}

snippet el "else block"
	else {
		${0}
	}

snippet whl "while block"
	while ${1:condition} {
		${0}
	}

snippet for "for loop"
	for ${1:pat} in ${2:expr} {
		${0}
	}

snippet lop "loop block"
	loop {
		${0}
	}

snippet ifl "if let"
	if let ${1:pat} = ${2:expr} {
		${0}
	}

snippet wll "while let"
	while let ${1:pat} = ${2:expr} {
		${0}
	}

snippet mat "match block"
	match ${1:expr} {
		${2} => ${0},
	}

snippet impl "impl block"
	impl ${1:type} {
		${0}
	}

snippet implt "impl trait"
	impl ${1:trait} for ${2:type} {
		${0}
	}

snippet tt "tarit"
	trait ${1:name} {
		${0}
	}

snippet mod "mod"
	mod ${0:name}

snippet modb "modblock"
	mod ${1:name} {
		${0}
	}

snippet tst "tuple struct"
	struct ${1:name}(${0:type});

snippet ust "unit struct"
	struct ${0:name};

snippet st "struct block"
	struct ${1:name} {
		${2:field}: ${0:type},
	}

snippet enm "enum block"
	enum ${1:name} {
		${2},
		${0}
	}

snippet tas "type alias"
	type ${1:alias} = ${0:type};

snippet der "derive"
	#[derive(${0})]

snippet allow "allow"
	#[allow(${0})]

snippet deny "deny"
	#[deny(${0})]

snippet mau "macro use"
	#[macro_use(${0})]

snippet mae "macro export"
	#[macro_export]

snippet cfg "#[cfg()]"
